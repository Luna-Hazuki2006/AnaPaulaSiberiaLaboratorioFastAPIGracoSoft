{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debugger: FastAPI path src",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "cwd": "src/",
            "args": ["main:app", "--reload"],
            "jinja": true
        },
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        }
    ]
}
